{
  "name": "sstimerv2",
  "icon": "./renderer/assets/clock.ico",
  "productName": "SSTimer (v2)",
  "version": "1.0.0",
  "description": "Lightweight desktop app that allows users to track summoner spells in league of legends. Inspired by Porofessor.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "buildw": "electron-packager . SSTimer --platform=win32 --arch=x64 --overwrite --icon=./renderer/assets/clock.ico --prune=true --out=release-builds",
    "buildm": "electron-packager . SSTimer --platform=darwin --arch=x64 --overwrite --icon=./renderer/assets/clock.ico --prune=true --out=release-builds"
  },
  "author": "deR0R0",
  "license": "MIT",
  "dependencies": {
    "electron": "^31.6.0"
  }
}
